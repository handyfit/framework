{
    "name": "handyfit/framework",
    "description": "Based on laravel derived development framework.",
    "license": "mit",
    "autoload": {
        "psr-4": {
            "Handyfit\\Framework\\": "src/"
        }
    },
    "authors": [
        {
            "name": "KanekiYuto",
            "email": "kaneki.yuto.404@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/handyfit/framework/issues",
        "source": "https://github.com/handyfit/framework"
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.9"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        },
        "laravel": {
            "providers": [
                "Handyfit\\Framework\\HandyFitServiceProvider"
            ],
            "aliases": {
                "Preacher": "Handyfit\\Framework\\Support\\Facades\\Preacher"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
